{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 90,
  "tabWidth": 2,
  "proseWrap": "always",
  "endOfLine": "lf",
  "useTabs": false,
  "requirePragma": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "insertPragma": false,
  "htmlWhitespaceSensitivity": "css",
  "bracketSpacing": true,
  "arrowParens": "avoid"
}
